    <script>
        kendo.culture("es-PE");
        $('.sis-net').on('click', function (e) {
            //alert(this.getAttribute("sis"));
           

            $.ajax({
                datatype: 'json',
                contentType: "application/json",
                url: '/Account/Redireccion',
                type: 'POST',
                data: JSON.stringify({ key: this.getAttribute("sis") }),
                beforeSend: function () {
                    bloquoteObject();
                },
                success: function (data) {
                    debugger;
                    console.log(data);
                    if (!data.rpta) {
                        AlertMessage(data.result);

                    } else {
                        //AlertMessage(data.result);
                        window.location.href = data.url;
                    }

                    desbloqObject();
                }
            }).fail(function (jqxhr, textStatus, error) {
                var err = textStatus + ', ' + error;
                console.log("Request Failed: " + err);
                desbloqObject();
            });
        });

        $(document).ready(function (e) {

            if ($('#fotocheck').text() == "") {
                $(nombreUsuario).val()
            } else {
                loadUser();
            }
            

            function loadUser() {
                var myHeaders = new Headers();
                myHeaders.append("Content-Type", "application/json");

                var raw = JSON.stringify({
                    Codigo: $('#fotocheck').text()
                })

                var requestOptions = {
                    method: 'POST',
                    headers: myHeaders,
                    body: raw,
                    redirect: 'follow'
                };
                fetch("/Operario/ListarUsuarioDatos", requestOptions)
                    .then(response => response.text())
                    //.then(result => console.log(result))
                    .then(function (datos) {
                        //HabilitarGrabado(1)
                        //console.log('datosssss')
                        console.log(datos)
                        
                        var data = JSON.parse(datos)
                        if (data.result) {
                            $(nombreUsuario).text(data.Datos.Descripcion)
                        }
                        desbloqObject()
                    })
                    //.catch(error => console.log('error', error));
                    .catch(function (error) {
                        console.log('error', error);
                        $('#txtSupervisorNombre').val('');
                        sessionStorage.setItem("SupervisorCodigo", '');
                        desbloqObject()
                    })
            }
        })
    </script>

